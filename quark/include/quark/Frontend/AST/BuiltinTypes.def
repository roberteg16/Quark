#ifndef QK_BUILTIN_TYPE
#define QK_BUILTIN_TYPE(TYPE, NAME, BYTES, SIGNED)
#endif

#ifndef QK_BUILTIN_INTEGER_TYPE
#define QK_BUILTIN_INTEGER_TYPE(TYPE, BYTES, SIGNED) QK_BUILTIN_TYPE(TYPE, TYPE, BYTES, SIGNED)
#endif

#ifndef QK_BUILTIN_FLOATING_TYPE
#define QK_BUILTIN_FLOATING_TYPE(TYPE, BYTES, SIGNED) QK_BUILTIN_TYPE(TYPE, TYPE, BYTES, SIGNED)
#endif

#ifndef QK_BUILTIN_VECTOR_TYPE
#define QK_BUILTIN_VECTOR_TYPE(TYPE, BYTES, SIGNED) QK_BUILTIN_TYPE(TYPE, TYPE, BYTES, SIGNED)
#endif

#ifndef QK_BUILTIN_VOID_TYPE
#define QK_BUILTIN_VOID_TYPE(TYPE) QK_BUILTIN_TYPE(TYPE, void, 1, false)
#endif

#ifndef QK_BUILTIN_BOOL_TYPE
#define QK_BUILTIN_BOOL_TYPE(TYPE) QK_BUILTIN_TYPE(TYPE, TYPE, 1, false)
#endif

QK_BUILTIN_VOID_TYPE(Void)
QK_BUILTIN_BOOL_TYPE(b1)

QK_BUILTIN_INTEGER_TYPE(u8,   1,  false)
QK_BUILTIN_INTEGER_TYPE(i8,   1,  true)
QK_BUILTIN_INTEGER_TYPE(u16,  2,  false)
QK_BUILTIN_INTEGER_TYPE(i16,  2,  true)
QK_BUILTIN_INTEGER_TYPE(u32,  3,  false)
QK_BUILTIN_INTEGER_TYPE(i32,  3,  true)
QK_BUILTIN_INTEGER_TYPE(u64,  4,  false)
QK_BUILTIN_INTEGER_TYPE(i64,  4,  true)

QK_BUILTIN_FLOATING_TYPE(f32, 1,  true)
QK_BUILTIN_FLOATING_TYPE(f64, 2,  true)
QK_BUILTIN_FLOATING_TYPE(f80, 4,  true)

QK_BUILTIN_VECTOR_TYPE(u128,  4,  false)
QK_BUILTIN_VECTOR_TYPE(i128,  4,  true)
QK_BUILTIN_VECTOR_TYPE(i256,  8,  true)
QK_BUILTIN_VECTOR_TYPE(u256,  8,  false)
QK_BUILTIN_VECTOR_TYPE(i512,  16, true)
QK_BUILTIN_VECTOR_TYPE(u512,  16, false)

#undef QK_BUILTIN_BOOL_TYPE
#undef QK_BUILTIN_VOID_TYPE
#undef QK_BUILTIN_VECTOR_TYPE
#undef QK_BUILTIN_FLOATING_TYPE
#undef QK_BUILTIN_INTEGER_TYPE
#undef QK_BUILTIN_TYPE
